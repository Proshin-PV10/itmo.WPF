<Window x:Class="WpfRoutedEvent_2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfRoutedEvent_2"
        mc:Ignorable="d"
        Title="MainWindow" Height="119" Width="604">
    <Grid Margin="0,0,0,-6">
        <TextBox x:Name="txtBox" HorizontalAlignment="Left" Margin="459,15,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <StackPanel ButtonBase.Click="StackPanel_Click" HorizontalAlignment="Left" Width="364" Height="72" VerticalAlignment="Top">
            <Button x:Name="butAdd" Content="*2"/>
            <Button x:Name="butMult" Content="^2"/>
        </StackPanel>

    </Grid>
</Window>
